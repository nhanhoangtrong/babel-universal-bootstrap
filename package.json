{
  "name": "kendy-universal-bootstrap",
  "version": "1.0.0",
  "description": "A Universal Starter Kit include Webpack 2, Express, React, Gulp tasks, and more ",
  "main": "index.js",
  "engines": {
    "node": "^8.10.0"
  },
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/config.server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "test:client": "jest --setupTestFrameworkScriptFile=\"<rootDir>/setupTests.client.js\"",
    "test:server": "jest --testEnvironment=node --testPathIgnorePatterns=\"(.*)\\.test\\.js\" --setupTestFrameworkScriptFile=\"<rootDir>/setupTests.server.js\"",
    "lint": "eslint --ext .js --ext .jsx --fix ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/siege250294/kendy-universal-bootstrap.git"
  },
  "keywords": [
    "universal",
    "react",
    "redux",
    "gulp",
    "express",
    "isomorphism"
  ],
  "author": "siege250294",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siege250294/kendy-universal-bootstrap/issues"
  },
  "homepage": "https://github.com/siege250294/kendy-universal-bootstrap#readme",
  "dependencies": {
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "immutable": "^3.8.1",
    "prop-types": "^15.6.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "chalk": "^2.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "jest": "^22.4.3",
    "mocha": "^4.0.0",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.0",
    "postcss-loader": "^2.0.0",
    "raf": "^3.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.0",
    "supertest": "^3.0.0",
    "webpack": "^3.8.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-node-externals": "^1.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "@app(.*)$": "<rootDir>/src$1"
    }
  }
}
